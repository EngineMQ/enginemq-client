{
  "name": "engine-mq-client",
  "description": "Engine-MQ Message queue Client lib",
  "author": "BCsabaEngine <csaba@balazs-magyar.hu>",
  "license": "MIT",
  "version": "1.0.2",
  "main": "./index.js",
  "types": "./index.d.ts",
  "engines": {
    "node": ">=16",
    "npm": "~8"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Engine-MQ/engine-mq-client"
  },
  "bugs": {
    "url": "https://github.com/Engine-MQ/engine-mq-client/issues"
  },
  "homepage": "https://github.com/Engine-MQ/engine-mq-client",
  "scripts": {
    "example-subscribe": "nodemon examples/simpleClient.ts -- subscribe",
    "example-publish": "nodemon examples/simpleClient.ts -- publish",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "tsc --build --clean",
    "build": "tsc --build --clean && tsc --build --force",
    "npm-publish": "npm run build && npm version patch && npm publish && npm run clean"
  },
  "keywords": [
    "engine-mq",
    "message queue",
    "mqueue"
  ],
  "dependencies": {
    "@sinclair/typebox": "^0.23.4",
    "@types/semver": "^7.3.9",
    "ajv": "^8.10.0",
    "msgpackr": "^1.5.4",
    "pretty-ms": "^7.0.1",
    "semver": "^7.3.5",
    "short-uuid": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}